{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "More-Recipes provides a platform for users to share the awesome and exciting recipe ideas they have invented or learnt.\r Suppose a user comes up with a recipe, he/she can post it on More-Recipes and get feedback in form of reviews and votes from other users who explore that recipe. Users can also keep a list of their favorite recipes on the application.",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon --exec babel-node server/bin/www",
    "test": "mocha server/test/*.js --exit --require babel-register --reporter spec",
    "build": "babel ./server/ -d dist/server",
    "start": "babel-node server/bin/www",
    "heroku-postbuild": "npm run build",
    "migrate": "sequelize db:migrate:undo:all&&sequelize db:migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "build:client": "webpack",
    "dev": "webpack-dev-server",
    "clean":  "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/missating/More-Recipes.git"
  },
  "author": "Vanessa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/missating/More-Recipes/issues"
  },
  "homepage": "https://github.com/missating/More-Recipes#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sequelize": "^4.19.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
